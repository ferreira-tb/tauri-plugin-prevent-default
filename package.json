{
  "name": "tauri-plugin-prevent-default",
  "version": "0.2.0",
  "description": "Disable default webview shortcuts",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@9.6.0",
  "homepage": "https://github.com/ferreira-tb/tauri-plugin-prevent-default",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ferreira-tb/tauri-plugin-prevent-default.git"
  },
  "bugs": {
    "url": "https://github.com/ferreira-tb/tauri-plugin-prevent-default/issues"
  },
  "contributors": [
    {
      "name": "Andrew Ferreira",
      "email": "andrew.shien2@gmail.com",
      "url": "https://tb.dev.br/"
    }
  ],
  "keywords": [
    "tauri",
    "plugin"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,md}": "prettier --write"
  },
  "scripts": {
    "build": "vite build",
    "clippy": "cargo clippy",
    "format": "prettier . --write && cargo fmt",
    "prepare": "husky",
    "test": "cargo test --lib",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0-beta.15"
  },
  "devDependencies": {
    "@tb-dev/eslint-config": "^4.4.2",
    "@types/node": "^20.14.12",
    "eslint": "^8.57.0",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-dts": "^3.9.1"
  }
}
